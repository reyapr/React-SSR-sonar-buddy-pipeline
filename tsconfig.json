{
  "compilerOptions": {	    
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
  }
}